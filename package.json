{
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.38.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^16.2.6",
    "posthog-js": "^1.258.6",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "scripts": {
    "lint": "eslint",
    "format:check": "prettier --check src/",
    "format:write": "prettier --write src/",
    "postinstall": "husky",
    "prepare": "husky install"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
